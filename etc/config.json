{
	"directory": "/home/mstudd/tmp/repo",
	"os": {
		"version": "6",
		"test": "6.7",
		"rc": "6.7",
		"prod": "6.7",
		"repositories": [ "os", "updates" ],
		"sync": "mirrors.rit.edu::centos/$rel/$repo/$arch"
