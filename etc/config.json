		{
			"name": "local",
			"upstream": [ "recognia" ]
		}
	],
	"upstream": [
		{
			"name": "epel",
			"sync": "mirrors.mit.edu::fedora-epel/$rel/$arch",
			"restrict": [ "FIXME" ]
		},
		{
			"name": "pgrpm-92",
			"sync": "yum.postgresql.org::$repo/redhat/rhel-$rel-$arch"
		}
	]
